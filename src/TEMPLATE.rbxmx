								<ProtectedString name="Source"><![CDATA[local Mouse 	= game.Players.LocalPlayer:GetMouse()

local UILocked	= script.Parent.Parent.Parent:WaitForChild("UILocked")

-- controls focus effect
local Slider = script.Parent
local SliderFG	= Slider:WaitForChild("fg")

local FG_COLOR_ON 			= Color3.fromRGB(68, 171, 218)
local FG_COLOR_OFF			= Color3.fromRGB(47, 161, 214)

local BG_COLOR_ON 			= Color3.fromRGB(60, 60, 60)
local BG_COLOR_OFF 			= Color3.fromRGB(48, 48, 48)

local isFocused = false

Slider.MouseEnter:Connect(function()
	if UILocked.Value ~= "ACTIVE" then
		return
	end
	
	Slider.BackgroundColor3 = BG_COLOR_ON
	SliderFG.BackgroundColor3 = FG_COLOR_ON
end)

Slider.MouseMoved:Connect(function()
	if UILocked.Value ~= "ACTIVE" then
		return
	end
	
	Slider.BackgroundColor3 = BG_COLOR_ON
	SliderFG.BackgroundColor3 = FG_COLOR_ON
end)

Slider.MouseLeave:Connect(function()
	Slider.BackgroundColor3 = BG_COLOR_OFF
	SliderFG.BackgroundColor3 = FG_COLOR_OFF
end)]]></ProtectedString>
