							<ProtectedString name="Source"><![CDATA[
local Controller = script.Parent.Parent
local Value 	= Controller:WaitForChild("Value")
local UILocked	= Controller:WaitForChild("UILocked")

local Text 		= script.Parent:WaitForChild("text-container"):WaitForChild("text"):WaitForChild("text")

local controllerHover	= false
local textFocused 		= false

function checkUnlock()	
	if controllerHover or textFocused then
		return
	end
	
	spawn(function()
		UILocked.Value = "UNLOCK"
	end)
end

Controller.MouseEnter:Connect(function()
	if UILocked.Value ~= "ACTIVE" then
		return
	end
	
	controllerHover = true
end)

Controller.MouseMoved:Connect(function()
	if UILocked.Value ~= "ACTIVE" then
		return
	end
	
	controllerHover = true
end)

Controller.MouseLeave:Connect(function()
	controllerHover = false
	checkUnlock()
end)

Text.Focused:Connect(function()
	if UILocked.Value ~= "ACTIVE" then
		return
	end
	
	textFocused = true
end)

-- On change text by user
Text.FocusLost:Connect(function(enterPressed, inputObject)
	if UILocked.Value ~= "ACTIVE" then
		Text.Text = Value.Value
	else
		Value.Value = Text.Text
	end	
	
	textFocused = false
	
	checkUnlock()
end)

UILocked.Changed:connect(function()
	if UILocked.Value ~= "ACTIVE" then
		-- reset when external lock (eg close folder)		
		controllerHover	= false
		textFocused		= false
		Text.Active = false
		Text.Selectable = false
		Text.TextEditable = false
	else
		Text.Active = true
		Text.Selectable = true
		Text.TextEditable = true
	end
end)

-- On change value (safe)
Value.Changed:connect(function()
	Text.Text = Value.Value
end)
]]></ProtectedString>
