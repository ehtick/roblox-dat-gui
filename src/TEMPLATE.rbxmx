local UILocked		= script.Parent.Parent.Parent.Parent:WaitForChild("UILocked")

-- controls focus effect
local TextFrame 	= script.Parent
local Text 			= TextFrame:WaitForChild("text")

local COLOR_TEXT_ON 		= Color3.fromRGB(255, 255, 255)
local COLOR_TEXT_OFF		= Color3.fromRGB(30, 211, 111)
local COLOR_TEXT_BG_ON 		= Color3.fromRGB(73, 73, 73)
local COLOR_TEXT_BG_OFF 	= Color3.fromRGB(48, 48, 48)
local COLOR_TEXT_BG_HOVER	= Color3.fromRGB(60, 60, 60)

local textFocused = false

Text.MouseEnter:Connect(function()
	if UILocked.Value ~= "ACTIVE" then
		return
	end
	
	if textFocused == false then
		TextFrame.BackgroundColor3 = COLOR_TEXT_BG_HOVER
	end
end)

Text.MouseLeave:Connect(function()
	if textFocused == false then
		TextFrame.BackgroundColor3 = COLOR_TEXT_BG_OFF
	end
end)

Text.Focused:Connect(function(enterPressed, inputObject)
	if UILocked.Value ~= "ACTIVE" then
		return
	end
	
	textFocused = true
	Text.TextColor3 = COLOR_TEXT_ON
	Text.PlaceholderColor3 = COLOR_TEXT_ON
	TextFrame.BackgroundColor3 = COLOR_TEXT_BG_ON	
end)

Text.FocusLost:Connect(function(enterPressed, inputObject)
	textFocused = false
	Text.TextColor3 = COLOR_TEXT_OFF
	Text.PlaceholderColor3 = COLOR_TEXT_OFF
	TextFrame.BackgroundColor3 = COLOR_TEXT_BG_OFF
end)

-- reset when external lock (eg close folder)
UILocked.Changed:connect(function()
	if UILocked.Value == "LOCKED" then
		textFocused = false
		Text.TextColor3 = COLOR_TEXT_OFF
		Text.PlaceholderColor3 = COLOR_TEXT_OFF
		TextFrame.BackgroundColor3 = COLOR_TEXT_BG_OFF
	end
end)
]]></ProtectedString>
