ValueIn.Changed:connect(function()
	local value = math.max( math.min(ValueIn.Value, Max.Value), Min.Value)
	
	if value % Step.Value ~= 0 then
		value = math.round(value/Step.Value) * Step.Value
	end
	
	Value.Value = value
end)

