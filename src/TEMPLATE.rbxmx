-- Services
local UserInputService = game:GetService("UserInputService")

local Util 				= require(script.Parent:WaitForChild("util"))
local UILocked			= script.Parent.Parent:WaitForChild("UILocked")

-- values
local Hue 				= script.Parent:WaitForChild("Hue")
local Saturation 		= script.Parent:WaitForChild("Saturation")
local Luminance 		= script.Parent:WaitForChild("Luminance")
local Color 			= script.Parent.Parent:WaitForChild("Value")

-- ui
local Controller		= script.Parent.Parent
local TextContainer 	= script.Parent:WaitForChild("text-container")
local Text 				= TextContainer:WaitForChild("text")

local Selector 			= script.Parent:WaitForChild("selector")
local SatLumSelector 	= Selector:WaitForChild("saturation")
local SatLumKnob 		= SatLumSelector:WaitForChild("knob")
local SaturationImage 	= SatLumSelector:WaitForChild("saturation")

local HueSelector 		= Selector:WaitForChild("hue")
local HueKnob 			= HueSelector:WaitForChild("knob")

local controllerHover 	= false
local selectorHover 	= false
local hueHover 			= false
local hueMouseDown 		= false
local satLumHover 		= false
local satLumMouseDown 	= false
local textHover 		= false
local textFocused 		= false

local function changeColor()
	
	local hue, sat, lum = Color.Value:ToHSV()	
	
	local changed = false
	
	if math.floor(239*hue+0.5) ~= Hue.Value then
		hue = Hue.Value/239
		changed = true
	end
	
	if math.floor(240*sat+0.5) ~= Saturation.Value  then
		sat = Saturation.Value/240
		changed = true
	end
	
	if  math.floor(240*lum+0.5) ~= Luminance.Value  then
		lum = Luminance.Value/240
		changed = true
	end
	
	if changed then
		Color.Value = Color3.fromHSV(hue, sat, lum)
	end
end

local function renderText()
	local color = Color.Value
	
	TextContainer.BackgroundColor3 = color	
	
	Text.TextColor3 = Util.bestContrast(color)		
	Text.Text = string.format("#%.2X%.2X%.2X", color.R * 255, color.G * 255, color.B * 255)
end

function changeSelector()
	local hue, sat, lum  = Color.Value:ToHSV()	
	
	Hue.Value 			= math.floor(239 * hue + 0.5)	
	Saturation.Value	= math.floor(240 * sat + 0.5)
	Luminance.Value		= math.floor(240 * lum + 0.5)
	
	SatLumKnob.Position 			= UDim2.new(sat, -5, 1-lum , -5)
	SatLumKnob.BorderColor3 		= Util.bestContrast(Color.Value)	
	SatLumKnob.BackgroundColor3 	= Color.Value	
	
	renderText()	
end

Color.Changed:connect(changeSelector)

local function color3FromString(hex) 
	local r, g, b, hash
	if string.len(hex) == 6 then
		-- FFFFFF
		r, g, b  = hex:match("(..)(..)(..)")
	elseif string.len(hex) == 7 then
		-- #FFFFFF
		hash, r, g, b  = hex:match("(.)(..)(..)(..)")
	elseif string.len(hex) == 3 then
		-- FFF
		r, g, b  = hex:match("(.)(.)(.)")
		r = r..r
		g = g..g
		b = b..b
	elseif string.len(hex) == 4 then
		-- #FFF
		hash, r, g, b  = hex:match("(.)(.)(.)(.)")
		r = r..r
		g = g..g
		b = b..b
	else
		return nil
	end
	
	r, g, b = tonumber(r, 16), tonumber(g, 16), tonumber(b, 16)
	
	return Color3.fromRGB(r, g, b)
end

function checkVisibility()
	if UILocked.Value ~= "ACTIVE" then
		Selector.Visible = false
		
	elseif textHover or textFocused or selectorHover or  hueHover or satLumHover or hueMouseDown or satLumMouseDown then
		Selector.Visible = true
	else
		Selector.Visible = false
	end	
end

function checkUnlock()	
	checkVisibility()
	
	if controllerHover or textHover or textFocused or selectorHover or hueHover or satLumHover or hueMouseDown or satLumMouseDown then
		return
	end
	
	Selector.Visible = false
	
	spawn(function()		
		UILocked.Value = "UNLOCK"
	end)
end

Hue.Changed:connect(function()
	local hue = Hue.Value
	if hue == 0 then
		hue = 1
	end
	
	HueKnob.Position = UDim2.new(1, -2, 1-hue/239, -1)	
	SaturationImage.ImageColor3 = Color3.fromHSV(Hue.Value/239, 1, 1)
	
	changeColor()
end)

Saturation.Changed:connect(function()
	changeColor()
end)

Luminance.Changed:connect(function()
	changeColor()
end)

Selector.MouseEnter:Connect(function()
	if UILocked.Value ~= "ACTIVE" then
		return
	end
	
	selectorHover = true
	checkVisibility()
end)

Selector.MouseMoved:Connect(function()
	if UILocked.Value ~= "ACTIVE" then
		return
	end
	
	selectorHover = true
	checkVisibility()
end)

Selector.MouseLeave:Connect(function()
	selectorHover = false
	checkUnlock()
end)

Controller.MouseEnter:Connect(function()
	if UILocked.Value ~= "ACTIVE" then
		return
	end
	
	controllerHover = true
	checkVisibility()
end)

Controller.MouseMoved:Connect(function()
	if UILocked.Value ~= "ACTIVE" then
		return
	end
	
	controllerHover = true
	checkVisibility()
end)

Controller.MouseLeave:Connect(function()
	controllerHover = false
	checkUnlock()
end)

SatLumSelector.MouseEnter:Connect(function()
	if UILocked.Value ~= "ACTIVE" then
		return
	end
	
	satLumHover = true
	checkVisibility()
end)

SatLumSelector.MouseMoved:Connect(function()
	if UILocked.Value ~= "ACTIVE" then
		return
	end
	
	satLumHover = true
	checkVisibility()
end)

SatLumSelector.MouseLeave:Connect(function()
	satLumHover = false
	checkUnlock()
end)

HueSelector.MouseEnter:Connect(function()
	if UILocked.Value ~= "ACTIVE" then
		return
	end
	
	hueHover = true
	checkVisibility()
end)

HueSelector.MouseMoved:Connect(function()
