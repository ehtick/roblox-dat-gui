-- Services
local UserInputService = game:GetService("UserInputService")

local Controller	= script.Parent
local UILocked		= Controller:WaitForChild("UILocked")
local LabelValue	= Controller:WaitForChild("Label")
local LabelText		= Controller:WaitForChild("LabelText")
local OnClickEvent	= Controller:WaitForChild("OnClick")

Controller.MouseLeave:Connect(function()	
	spawn(function()
		UILocked.Value = "UNLOCK"
	end)
end)

UserInputService.InputEnded:Connect(function(input, gameProcessed)
	if gameProcessed then
		return
	end
	if UILocked.Value == "ACTIVE" and input.UserInputType == Enum.UserInputType.MouseButton1 then
		OnClickEvent:Fire()
	end
end)

LabelValue.Changed:connect(function()
	LabelText.Text = LabelValue.Value
end)

-- init
LabelText.Text = LabelValue.Value

]]></ProtectedString>
