							<ProtectedString name="Source"><![CDATA[local HttpService = game:GetService("HttpService")

-- Services
local UserInputService = game:GetService("UserInputService")

-- "Public" Values
local Options 		= script.Parent.Parent:WaitForChild("Options")
local UILocked		= script.Parent.Parent:WaitForChild("UILocked")
local Selected 		= script.Parent.Parent:WaitForChild("Selected")		-- safe value
local SelectedIn 	= script.Parent.Parent:WaitForChild("SelectedIn")	-- unsafe value

-- Elements
local Controller		= script.Parent.Parent
local SelectContainer	= script.Parent:WaitForChild("items-container")
local SelectList		= SelectContainer:WaitForChild("inner")
local ItemTemplate 		= SelectContainer:WaitForChild("item")

-- hide
ItemTemplate.Visible = false

-- variables
local itemsSize			= 0
local controllerHover 	= false
local selectListHover 	= false
local selectListIsOpen	= false

function checkVisibility()
	if UILocked.Value == "LOCKED" then
		selectListIsOpen = false
		SelectList.Size = UDim2.new(1, 0, 1, 0)
		
	else
		SelectList.Size = UDim2.new(1, 0, 0, itemsSize)		
	end
	
	if UILocked.Value == "ACTIVE" and selectListIsOpen then
		
		SelectContainer.ZIndex = 100
		SelectContainer.ClipsDescendants = false		
		
		for _, item in pairs(SelectList:GetChildren()) do
			if item:isA("Frame") then
				item.Visible = true
			end
		end
	else
		SelectContainer.ZIndex = 1		
		SelectContainer.ClipsDescendants = true		
		
		for index, item in pairs(SelectList:GetChildren()) do
			if item:isA("Frame") then
				if item.Name ==  "item_"..Selected.Value then
					item.Visible = true
				else
					item.Visible = false
				end
			end
		end
	end
end

function checkUnlock()	
	checkVisibility()
	
	if controllerHover or selectListHover then
		return
	end
	
	spawn(function()		
		UILocked.Value = "UNLOCK"
	end)
end

Controller.MouseEnter:Connect(function()
	if UILocked.Value ~= "ACTIVE" then
		return
	end
	
	controllerHover = true
	checkVisibility()
end)

Controller.MouseMoved:Connect(function()
	if UILocked.Value ~= "ACTIVE" then
		return
	end
	
	controllerHover = true
	checkVisibility()
end)

Controller.MouseLeave:Connect(function()
	controllerHover = false
	checkUnlock()
end)

SelectList.MouseEnter:Connect(function()
	if UILocked.Value ~= "ACTIVE" then
		return
	end
	
	selectListHover = true
	checkVisibility()
end)

SelectList.MouseMoved:Connect(function()
	if UILocked.Value ~= "ACTIVE" then
		return
	end
	
	selectListHover = true
	checkVisibility()
end)

SelectList.MouseLeave:Connect(function()
	selectListHover = false
	selectListIsOpen = false
	
	checkUnlock()
end)

-- On change value (safe)
Selected.Changed:connect(function()
	checkVisibility()
end)

Options.Changed:Connect(function()
	local labels = HttpService:JSONDecode(Options.Value)
	
	local size = 0
	for index, label in pairs(labels) do
		local item = ItemTemplate:Clone()		
		item.Name = "item_"..index
		item.Visible = true
		item.Parent = SelectList
		item.LayoutOrder = index
		
		-- enable script
		item:WaitForChild("script").Disabled = false
		
		local Text = item:WaitForChild("text")
		Text.Text = label
		
		size += Text.AbsoluteSize.Y
		
		Text.MouseButton1Click:Connect(function()
			if selectListIsOpen then				
				Selected.Value = index
				selectListIsOpen = false
				checkUnlock()
				
			else
				if UILocked.Value ~= "ACTIVE" then
					return
				end
				
				selectListIsOpen = true
				checkVisibility()
			end
		end)
	end
	
	itemsSize = size
	checkVisibility()
end)

UILocked.Changed:connect(function()
	if UILocked.Value == "LOCKED" then
		-- reset when external lock (eg close folder)
		controllerHover 	= false
		selectListHover 	= false
		selectListIsOpen	= false
	end
	
	checkVisibility()
end)

-- Selected.Value = 1
checkVisibility()
]]></ProtectedString>
