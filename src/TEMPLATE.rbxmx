							<ProtectedString name="Source"><![CDATA[-- Services
local UserInputService = game:GetService("UserInputService")

-- "Public" Values
local Value 	= script.Parent.Parent:WaitForChild("Value")

-- Elements
local UILocked	= script.Parent.Parent:WaitForChild("UILocked")
local Control 	= script.Parent
local Checkbox 	= Control:WaitForChild("checkbox")
local Image		= Checkbox:WaitForChild("check")

local BG_COLOR_ON 			= Color3.fromRGB(47, 161, 214)
local BG_COLOR_OFF 			= Color3.fromRGB(60, 60, 60)
local BG_COLOR_HOVER 		= Color3.fromRGB(48, 48, 48)

-- variables
local hover = false

function changeColor()
	if Value.Value then
		Checkbox.BackgroundColor3 = BG_COLOR_ON
	elseif hover then
		Checkbox.BackgroundColor3 = BG_COLOR_HOVER
	else
		Checkbox.BackgroundColor3 = BG_COLOR_OFF
	end
end

-- On change value (safe)
Value.Changed:connect(function()
	Image.Visible = Value.Value
	changeColor()
end)

Control.MouseEnter:Connect(function()
	if UILocked.Value ~= "ACTIVE" then
		return
	end
	
	hover = true
	changeColor()
end)

Control.MouseMoved:Connect(function()
	if UILocked.Value ~= "ACTIVE" then
		return
	end
	
	hover = true
	changeColor()
end)

Control.MouseLeave:Connect(function()	
	hover = false
	changeColor()
end)

UserInputService.InputBegan:Connect(function(input, gameProcessed)
	if UILocked.Value == "ACTIVE" and input.UserInputType == Enum.UserInputType.MouseButton1 then
		Value.Value = not Value.Value
	end
end)

-- reset when external lock (eg close folder)
UILocked.Changed:connect(function()
	if UILocked.Value == "LOCKED" then		
		hover = false
		changeColor()
	end
end)

-- init
Image.Visible = Value.Value
]]></ProtectedString>
